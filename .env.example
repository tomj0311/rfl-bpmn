# Environment variables for BPMN Editor
# Copy this file to .env.local and customize values

# Application settings
VITE_APP_NAME=BPMN Editor
VITE_APP_VERSION=1.0.0

# Security settings
VITE_MAX_FILE_SIZE=5242880  # 5MB in bytes
VITE_MAX_NODES_PER_DIAGRAM=1000
VITE_RATE_LIMIT_REQUESTS=10
VITE_RATE_LIMIT_WINDOW=60000  # 1 minute in milliseconds

# Feature flags
VITE_ENABLE_AUTO_SAVE=true
VITE_ENABLE_COLLABORATION=false
VITE_ENABLE_ANALYTICS=false

# Development settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=warn
